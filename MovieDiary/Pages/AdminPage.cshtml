@page
@model MovieDiary.Pages.AdminPageModel
@{
    ViewData["Title"] = "Admin Page";
}

<div class="admin-wrapper">
    @{
        foreach (var userName in Model.UserNames)
        {
            <form method="post" class="container">
                <div style="height:min-content; font-size:40px; border:3px solid #000000; width:500px; text-align:end">@userName</div>
                <button type="submit"><img src="~/icons/trash_bin.jpg" height="50px" width="50px"></button>
            </form>
        }
    }
</div>
