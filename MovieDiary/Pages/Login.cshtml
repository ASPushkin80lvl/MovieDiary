@page
@model MovieDiary.Pages.LoginModel
@{
    ViewData["Title"] = "Login page";
}

<form method="post" class="login-wrapper" style="flex-direction: column; display: flex">
    <input type="text" name="login" placeholder="Login" required>
    <input type="password" name="login" placeholder="Password" required>
    <input type="submit" value="Log In">
    <input type="submit" value="Sign Up With Google">
</form>
